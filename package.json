      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "my-app",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "extends": null
  },
  "devDependencies": {
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "electron-is-dev": "^2.0.0"
  }
